    <!-- Introduction information regarding Folding@Home -->
    <string name="wizard_welcome_to_folding_subtitle" xliff:restype="x-_UNLIMITED">Do you want to help find the cure for Alzheimer\'s, Parkinson\'s and cancer?</string>
    <!-- Complementary information regarding Folding@Home -->
    <string name="wizard_welcome_to_folding_text" xliff:restype="x-_UNLIMITED">Your phone can perform scientific calculations while you sleep, and help Stanford University scientists find the cure for many diseases.</string>
    <!-- List of text containing more information regarding Folding@Home project -->
    <string-array name="help_find_cure" xliff:restype="x-_UNLIMITED">

        <!-- Information about what you are contributing to when you join Folding@Home Project (Folding@Home should not be translated) -->
        <item>By signing up for Folding@Home, you help Stanford University scientists in their quest to find the cure for many of our most common diseases by donating your phone\'s processing power when you aren\'t using it yourself.</item>
        <!-- More information regarding the meaning of Folding and how the application helps people -->
        <item>Folding refers to protein activities in our bodies. The research of protein folding requires so many calculations that Stanford University asks people to donate their phones\' unused processing power to crunch some of the numbers. When you sleep, Stanford University will send a folding problem for your phone to solve. Once calculations are complete, the results are sent back to Stanford.</item>
        <!-- Link to Folding@Home homepage if the user wants to read more information. (Folding@Home and folding.stanford.edu should not be translated) -->
        <item>You can find more information about Folding@Home on the homepage:\nfolding.stanford.edu</item>
    </string-array>
    <!-- Text placed above the sponsors names: 'powered by Sony, Stanford University, Vodafone' -->
    <string name="powered_by" xliff:restype="x-GENERIC/LIST/SUBTITLE">powered by</string>
    <!-- Title label to let the user know that all the personal data is safe. -->
    <string name="private_date_safe_title" xliff:restype="x-GENERIC/LIST/TITLE/ICON">Your private data is safe.</string>
    <!-- Subtitle label, to let the user know that other applications can't access the private data. -->
    <string name="private_date_safe_text" xliff:restype="x-GENERIC/LIST/SUBTITLE/ICON">This application doesn\'t access your private data.</string>
    <!-- Title label, to let the user know that the transmitted/received data is free of charge, since it will use Wi-Fi -->
    <string name="no_cost_mobile_data_title" xliff:restype="x-GENERIC/LIST/TITLE/ICON">There is no cost for mobile data</string>
    <!-- Subtitle label, to let the user know that the phone must be connected to a wi-fi network. -->
    <string name="no_cost_mobile_data_text" xliff:restype="x-GENERIC/LIST/SUBTITLE/ICON">Phone must be connected to Wi-Fi network.</string>

    <!-- Title label, to let the user know that the battery will not discharge during the processing/calculation -->
    <string name="battery_not_drained_title" xliff:restype="x-GENERIC/LIST/TITLE/ICON">The battery will not be drained</string>
    <!-- Subtitle label, to let the user know that the the phone must be connected to a wall charger and the battery must be fully charged (100%) -->
    <string name="battery_not_drained_text" xliff:restype="x-GENERIC/LIST/SUBTITLE/ICON">The phone must be connected to a charger and fully charged (100%)</string>

    <!-- The three variables are the following: number of weeks, number of days and number of hours, all followed by the respectively abbreviated form ('w', 'd' and 'h'). e.g. '2w 5d 20h' -->
    <string name="time_string_weeks" xliff:restype="x-_UNLIMITED">%1$dw %2$dd %3$dh</string>
    <!-- The three variables are the following: number of days, number of hours and number of minutes, all followed by the respectively abbreviated form ('d', 'h' and 'm'). e.g. '5d 19h 59m' -->
    <string name="time_string_days" xliff:restype="x-_UNLIMITED">%1$dd %2$dh %3$dm</string>
    <!-- The two variables are the following: number of hours and number of minutes, all followed by the respectively abbreviated form ('h' and 'm'). e.g. '23h 59m' -->
    <string name="time_string_hours" xliff:restype="x-_UNLIMITED">%1$dh %2$dm</string>
